(export FLOWCELL_PATH=$(echo $(pwd) | cut -d "/" -f1,2,3,4,5);echo $FLOWCELL_PATH
export FLOWCELL=$(echo $(pwd) | cut -d "/" -f5);echo $FLOWCELL

for i in ${FLOWCELL_PATH}/Sample*;do cd ${FLOWCELL_PATH}/"$i"/;source ${FLOWCELL_PATH}/preprocessing/preprocess; wait;cd ${FLOWCELL_PATH};done
)>"run_preprocess.log" 2>&1